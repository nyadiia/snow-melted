# combining characters
<Multi_key> <period> <parenleft> : "ÃØ"
<Multi_key> <period> <bracketleft> : "Ã™" # dental
<Multi_key> <period> <bracketright> : "Ã∫" # apical
<Multi_key> <period> <braceleft> : "Ãª" # laminal
<Multi_key> <period> <braceright> : "Ãº" # linguolabial
<Multi_key> <period> <colon> : "Ã§" # breathy
<Multi_key> <period> <h> : "Ã•" # voiceless
<Multi_key> <period> <j> : "Ã¨" # voiced
<Multi_key> <period> <asciitilde> : "Ã∞" # creaky

